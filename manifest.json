{
    "name":"Basic Weather app",
    "description":"App that displays weather",
    "version":"0.0.1",
    "manifest_version":2,
    "browser_action":{
        "default_popup":"./popup.html",
        "default_icon":"./icons/128.png"
    }, 
    "icons":{
        "128":"./icons/128.png",
        "48":"./icons/48.png",
        "32":"./icons/32.png",
        "16":"./icons/16.png"
    },
    "permissions":["activeTab", "geolocation"]

}